{
  "name": "cesnet-spark",
  "version": "1.2.2",
  "author": "cesnet",
  "summary": "Apache Spark",
  "license": "MIT",
  "source": "https://github.com/MetaCenterCloudPuppet/cesnet-spark",
  "project_page": "https://github.com/MetaCenterCloudPuppet/cesnet-spark",
  "issues_url": "https://github.com/MetaCenterCloudPuppet/cesnet-spark/issues",
  "tags": ["big-data", "hadoop", "spark"],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 1.0.0 <7.0.0"
    },
    {
      "name": "cesnet/hadoop",
      "version_requirement": ">= 3.0.0 <4.0.0"
    },
    {
      "name": "cesnet/hadoop_lib",
      "version_requirement": "<1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6", "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7", "8", "9", "10"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6", "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "6", "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04", "16.04", "18.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.4.0"
    }
  ]
}
